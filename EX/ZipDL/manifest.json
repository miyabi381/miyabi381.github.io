{
  "manifest_version": 3,
  "name": "ZipDL",
  "version": "1.0",
  "description": "ブラウザからZipファイルURLを取得DLしてくれる！",
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "scripting",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "media/icon16.png",
    "32": "media/icon32.png",
    "48": "media/icon48.png",
    "128": "media/icon128.png"
  },
  "action": {
    "default_title": "ZipDL",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "media/icon16.png",
      "32": "media/icon32.png"
    }
  }
}